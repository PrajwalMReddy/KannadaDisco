{
  "0": {
    "english": "Move to the side.",
    "kannada": "ಸೈಡ್ಗೆ ಹಾಕಿ",
    "transliteration": "sidege hāki"
  },
  "1": {
    "english": "Tell me, sir.",
    "kannada": "ಹೇಳಿ ಸಾರ್",
    "transliteration": "heḷi sār"
  },
  "3": {
    "english": "Show me your license.",
    "kannada": "ನಿಮ್ಮ ಲೈಸೆನ್ಸ್ ತೋರಿಸಿ",
    "transliteration": "nimma license thōrisi"
  },
  "4": {
    "english": "Here sir.",
    "kannada": "ತಗೊಳಿ ಸಾರ್",
    "transliteration": "tagōḷi sār"
  },
  "5": {
    "english": "You have to pay a fine.",
    "kannada": "ನೀವು ಫೈನ್ ಕಟ್ಟಬೇಕು",
    "transliteration": "nīvu fine kattabēku"
  },
  "6": {
    "english": "Why? What did I do?",
    "kannada": "ಯಾಕೆ ನಾನು ಏನು ಮಾಡ್ಡೆ",
    "transliteration": "yāke nānu yēnu mādḍe"
  },
  "7": {
    "english": "There is no U-turn here.",
    "kannada": "ಇಲ್ಲಿ ಯೂಟರ್ನ್ ಇಲ್ಲ",
    "transliteration": "illi yūṭarn illa"
  },
  "8": {
    "english": "This is a one way road.",
    "kannada": "ಈ ರಸ್ತೆ ಒನ್ ವೇ",
    "transliteration": "Ī raste on vē"
  },
  "9": {
    "english": "Sorry, I didn't know.",
    "kannada": "ಸಾರೀ ನನಗೆ ಗೊತ್ತಿರಲಿಲ್ಲ",
    "transliteration": "sārī nanage gottiralilla"
  },
  "10": {
    "english": "I'll pay the fine.",
    "kannada": "ನಾನು ಫೈನ್ ಕೊಡ್ತೀನಿ",
    "transliteration": "nānu phain koḍtīni"
  },
  "11": {
    "english": "It's done. You may go.",
    "kannada": "ಆಯಿತು ನೀವು ಹೋಗಬಹುದು",
    "transliteration": "ayitu nivu hogabahudu"
  }
}
